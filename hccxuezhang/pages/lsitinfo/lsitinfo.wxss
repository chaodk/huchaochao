/* pages/lsitinfo/lsitinfo.wxss */
.info-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  background-color: #fff;
  font-size: 36rpx;
  text-align: center;
  box-shadow: 0px 6px 6px #b1b2b1;
  margin-bottom: 25px;
}
.info-mian{
  width: 100%;
  background-color: #fff;
  margin-top: 10px;
  box-shadow: 0px 6px 6px #b1b2b1;
}
.info-mian-title{
  width: 100%;
  height: 35px;
  line-height: 35px;
  border-bottom: 1px solid #b1b2b1;
  font-size: 36rpx;
}
.info-mian-title text{
  padding-left: 20rpx;
}
.info-mian-cont{
  width: 100%;
  font-size: 30rpx;
  color: #333;
}
.info-mian-cont text{
  display: inline-block;
  margin: 10px 20rpx;
}
map{
  width: 90%;
  height: 500rpx;
  margin: 10px auto;
}
