/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  font-weight: 300;
} 
page{
  background-color: #f5f5f5;
}
/* 益处隐藏省略号 */
.mingshi-name{
 display: -webkit-box ;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:1; 
}
.muen{
  width: 100%;
  margin-top: 30px;
  box-shadow: 0px 6px 3px #b1b2b1;
  background-color: #fff;
}
.muen-item{
  width: 250rpx;
  height: 250rpx;
  /* background-color: #fff; */
  display: flex;
  flex-direction: column;
  float: left;
  text-align:center;
  justify-content: center;
}
.muen-item image{
  width: 70rpx;
  height: 70rpx;
  margin: 0 auto;
  margin-bottom: 10px;
}
.muen-item text{
  text-shadow: 3px 3px 2px #999;
}
/* pages/gps/gps.wxss */
.col{
  width: 70%;
  height: 50px;
  line-height: 50px;
  text-align: center;
  /* border:1px solid #b1b2b1; */
  display: block;
  margin: 0 auto;
  margin-top: 15px;
  border-radius: 5px;
  box-shadow: 6px 6px 3px #b1b2b1;
  background-color: #FFEE9B;
}
.list_main{
  width: 100%;
  display: flex;
  background-color: #fff;
  margin-top: 20px;
  flex-direction:column;
  box-shadow: 6px 6px 3px #b1b2b1;
}
.list_item{
  width: 90%;
  margin: 0 auto;
  height: 55px;
  line-height: 55px;
  border-bottom: 1px #efefef dotted;
  font-size: 30rpx;
}
